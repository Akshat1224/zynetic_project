{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ],
    "redirects": [
      { "source": "/favicon.ico", "destination": "/src/assets/favicon.ico", "permanent": true }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  